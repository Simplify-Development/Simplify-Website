, setQuestion3] = React.useState({ question: \"Do you believe you have been banned\", answer: \"\" })\r\n    const [question4, setQuestion4] = React.useState({ question: \"Why should we unban you\", answer: \"\" })\r\n\r\n    const growers = document.querySelectorAll(\".grow-wrap\");\r\n\r\n    growers.forEach((grower) => {\r\n        const textarea = grower.querySelector(\"textarea\");\r\n        textarea.addEventListener(\"input\", () => {\r\n            grower.dataset.replicatedValue = textarea.value;\r\n        });\r\n    });\r\n\r\n    React.useEffect(() => {\r\n        getUserDetails()\r\n            .then(({ data }) => {\r\n                setUser(data);\r\n                console.log(data)\r\n                setLoading(false);\r\n            }).catch((err) => {\r\n                console.error(err)\r\n                window.location.href = `https://simplify-code.com/api/auth/discord`\r\n            })\r\n    }, [])\r\n\r\n    async function checkHandler() {\r\n        if (checked === true) {\r\n            await setCheck(false)\r\n        } else if (checked === false) {\r\n            await setCheck(true)\r\n        }\r\n    }\r\n\r\n    function submitHandler(event) {\r\n        getUserDetails().then(async result => {\r\n            const content = {\r\n                app: [question1, question2, question3, question4],\r\n                reqs: checked,\r\n                id: result.data.discordId,\r\n                user: result.data.username,\r\n                tag: result.data.tag,\r\n                appType: 'Ban Appeal'\r\n            }\r\n\r\n            if (question1.answer.split(\" \").join(\"\").length < 1 || question2.answer.split(\" \").join(\"\").length < 1 || question3.answer.split(\" \").join(\"\").length < 1 || question4.answer.split(\" \").join(\"\").length < 1) {\r\n                return toast.error('Make sure you fill out all the questions')\r\n            }\r\n\r\n            function getApplications() {\r\n                return axios.ge